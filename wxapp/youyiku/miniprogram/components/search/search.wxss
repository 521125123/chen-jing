page{
  background: #f5f5f5;
}
page,view,image,input {
  display: block;
  box-sizing: border-box;
}
.header {
  padding: 20rpx;
  background: #ccbaba;
  display: flex;
  align-items: center;
}
.header .search {
  position: relative;
  width: 750rpx;
  height: 60rpx;
}
.header .search input {
  width: 100%;
  height: 100%;
}
.header .search image {
  position: absolute;
  top:16rpx;
  z-index: 999;
}
.header .search .show-type1 {
  width: 40rpx;
  height: 40rpx;
  left: 20rpx;
}
.header .search .show-type2 {
  width: 50rpx;
  height: 50rpx;
  right: 150rpx;
}
.header .search .show-type3 {
  width: 50rpx;
  height: 50rpx;
  right: 50rpx;
}